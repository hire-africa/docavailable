{
  "name": "docavailable-minimal",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "node scripts/fix-gradle-props.js || true",
    "start": "expo start --lan",
    "start:tunnel": "expo start --tunnel",
    "start:localhost": "expo start --localhost",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "build:android": "expo run:android --variant release",
    "build:android-debug": "expo run:android --variant debug",
    "build:apk": "cd android && gradlew.bat assembleRelease",
    "build:apk-debug": "cd android && gradlew.bat assembleDebug",
    "build:apk-bundle": "cd android && gradlew.bat bundleRelease",
    "build:apk-preview": "node -e \"process.env.NODE_ENV='production'; require('child_process').execSync('cd android && gradlew.bat assembleRelease', {stdio: 'inherit'})\"",
    "clean:android": "cd android && gradlew.bat clean",
    "build:cloud-preview": "eas build --platform android --profile preview",
    "build:cloud-production": "eas build --platform android --profile production",
    "build:cloud-development": "eas build --platform android --profile development",
    "eject": "expo eject",
    "test:webrtc": "node test-webrtc-complete.js",
    "test:webrtc-connection": "node test-webrtc-connection.js",
    "test:webrtc-session": "node test-webrtc-session.js",
    "test:backend": "node test-backend-endpoints.js",
    "start:webrtc-server": "node start-webrtc-server.js",
    "signaling:start": "cd backend && npm start",
    "signaling:dev": "cd backend && npm run dev"
  },
