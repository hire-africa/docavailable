{
  "expo": {
    "name": "DocAvailable",
    "slug": "docavailable",
    "owner": "xee777",
    "workflow": "managed",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4CAF50"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.docavailable.minimal",
      "scheme": "com.docavailable.minimal",
      "notification": {
        "icon": "./assets/images/notification-icon.png",
        "color": "#4CAF50"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera for video calls",
        "NSMicrophoneUsageDescription": "This app needs access to microphone for voice and video calls",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to share images in chat",
        "NSBluetoothAlwaysUsageDescription": "This app needs Bluetooth access for headset support during calls"
      }
    },
    "android": {
      "package": "com.docavailable.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "notification": {
        "icon": "./assets/images/notification-icon.png",
        "color": "#4CAF50",
        "androidMode": "default",
        "androidCollapsedTitle": "DocAvailable"
      },
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "newArchEnabled": false,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.ACCESS_WIFI_STATE",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.WAKE_LOCK",
        "android.permission.VIBRATE",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_MICROPHONE",
        "android.permission.FOREGROUND_SERVICE_CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "scheme": "com.docavailable.app",
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      "./plugins/withCallKeep",
      "./plugins/withMainActivityFlags",
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": false,
            "enableBridgelessArchitecture": false
          }
        }
      ],
      "./plugins/withDisableNewArch",
      "./plugins/withForceDisableNewArch"
    ],
    "extra": {
      "firebaseApiKey": "AIzaSyAfRKBWJ8otf8VEo5nVZv5cG7qDX-t4ksc"
    }
  }
}
