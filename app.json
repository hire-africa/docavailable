{
  "expo": {
    "name": "DocAvailable",
    "slug": "Doc_available",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "runtimeVersion": "1.1.3",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4CAF50"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.docavailable.minimal",
      "scheme": "com.docavailable.minimal",
      "notification": {
        "icon": "./assets/images/notification-icon.png",
        "color": "#4CAF50"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera for video calls",
        "NSMicrophoneUsageDescription": "This app needs access to microphone for voice and video calls",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to share images in chat",
        "NSBluetoothAlwaysUsageDescription": "This app needs Bluetooth access for headset support during calls"
      }
    },
    "android": {
      "package": "com.docavailable.app",
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "notification": {
        "icon": "./assets/images/notification-icon.png",
        "color": "#4CAF50",
        "androidMode": "default",
        "androidCollapsedTitle": "DocAvailable"
      },
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "newArchEnabled": false,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE",
        "RECORD_AUDIO",
        "CAMERA",
        "MODIFY_AUDIO_SETTINGS",
        "BLUETOOTH",
        "BLUETOOTH_CONNECT",
        "WAKE_LOCK",
        "VIBRATE",
        "POST_NOTIFICATIONS",
        "FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "scheme": "com.docavailable.app",
    "plugins": [
      "expo-updates",
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      "./plugins/withWebRTC",
      "./plugins/withCallKeep",
      "./plugins/withMainActivityFlags",
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": false,
            "enableBridgelessArchitecture": false
          }
        }
      ],
      "./plugins/withDisableNewArch",
      "./plugins/withForceDisableNewArch"
    ],
    "extra": {
      "firebaseApiKey": "AIzaSyAfRKBWJ8otf8VEo5nVZv5cG7qDX-t4ksc"
    }
  }
}