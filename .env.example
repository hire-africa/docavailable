# DocAvailable Environment Configuration
# Copy this file to .env and adjust values as needed

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://docavailable-3vbdv.ondigitalocean.app
EXPO_PUBLIC_LARAVEL_API_URL=https://docavailable-3vbdv.ondigitalocean.app

# WebRTC Configuration
EXPO_PUBLIC_WEBRTC_SIGNALING_URL=wss://docavailable.org/call-signaling
EXPO_PUBLIC_WEBRTC_CHAT_SIGNALING_URL=wss://docavailable.org/chat-signaling
EXPO_PUBLIC_WEBRTC_CHAT_SERVER_URL=https://docavailable.org

# App Configuration
EXPO_PUBLIC_APP_NAME=DocAvailable
EXPO_PUBLIC_APP_VERSION=1.0.0

# Feature Flags
EXPO_PUBLIC_USE_BACKEND_CHAT=true
EXPO_PUBLIC_USE_BACKEND_WALLET=true
EXPO_PUBLIC_USE_BACKEND_NOTIFICATIONS=true
EXPO_PUBLIC_USE_BACKEND_APPOINTMENTS=true
EXPO_PUBLIC_BACKEND_ENABLED=true

# Logging Configuration (Performance Optimization)
# Options: error, warn, info, debug, verbose
# For production: use 'warn' or 'error'
# For development: use 'info' or 'debug'
EXPO_PUBLIC_LOG_LEVEL=warn
EXPO_PUBLIC_ENABLE_VERBOSE_LOGGING=false
EXPO_PUBLIC_ENABLE_DEBUG_LOGGING=false

# Payment Gateway Configuration
PAYCHANGU_API_KEY=your_paychangu_api_key
PAYCHANGU_SECRET_KEY=your_paychangu_secret_key
PAYCHANGU_MERCHANT_ID=your_paychangu_merchant_id
PAYCHANGU_WEBHOOK_SECRET=your_paychangu_webhook_secret
PAYCHANGU_ENVIRONMENT=sandbox
EXPO_PUBLIC_PAYCHANGU_PUBLIC_KEY=your_paychangu_public_key

# Other API Keys
PAYMENT_API_KEY=your_payment_api_key
SMS_API_KEY=your_sms_api_key
EMAIL_API_KEY=your_email_api_key

# AI Service Configuration
DEEPSEEK_API_KEY=your_deepseek_api_key
OPENAI_API_KEY=your_openai_api_key